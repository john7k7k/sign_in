model FishMap {
  id Int  @id @default(autoincrement())
  fish Fish[]
}

model Fish{
  fishMap FishMap @relation(fields: [uid],references: [id])
  uid Int
  version Int @id @default(autoincrement())
  bc Float
  err Int
  active Int
}

model User{
  userId Int @id @default(autoincrement())
  username String @unique
  email String
  
}


